
public class Animal {
	String name;
	int size;
	public void eat()
	{
		System.out.println("I am an animal witj name "+name+" eating munch munch....");
		
	}
	public String getName() {
		return name;
	}
	public void setName(String n) {
		this.name = n;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int s) {
		this.size = s;
	}
	
}
